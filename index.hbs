<div id="homepageposts">
  {{#foreach posts}}
  <div class="homepage-post-wrapper">
    <div class="homepage-post-info-wrapper" style="background-image:url({{img_url feature_image}});">
      <div class="homepage-post-info">
        <a href={{url}}>
          <div class="homepage-post-title">
            {{title}}
          </div>
        </a>
        <div class="homepage-post-excerpt">
          {{excerpt}}
        </div>
      </div>
    </div>
  </div>
  {{/foreach}}
</div>
{{pagination}}
